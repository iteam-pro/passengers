{
	"name": "mindforce/passengers",
	"description": "CakePHP Users management plugin",
	"homepage": "http://mindforce.me",
	"keywords": ["cakephp", "ui"],
	"type": "cakephp-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "Denys Kyselov",
			"homepage": "https://mindforce.me"
		}
	],
	"support": {
		"issues": "https://github.com/mindforce/passengers/issues",
		"source": "https://github.com/mindforce/passengers"
	},
	"minimum-stability": "dev",
	"require": {
		"cakephp/cakephp": "~3.0",
		"mindforce/rear_engine": "*",
		"cakedc/search": "3.0.*-dev",
		"dereuromark/cakephp-tools": "dev-cake3",
		"cakephp/migrations": "*",
		"friendsofcake/authenticate": "dev-cake3",
		"cakephp/acl": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "*"
	},
	"autoload": {
		"psr-4": {
			"Passengers\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Passengers\\Test\\": "tests"
		}
	}

}
