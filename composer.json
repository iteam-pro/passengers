{
	"name": "mindforce/cakephp-passengers",
	"description": "CakePHP Users management plugin",
	"homepage": "http://mindforce.me",
	"keywords": ["cakephp", "ui", "users", "authentication", "authorization"],
	"type": "cakephp-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "Denys Kyselov",
			"homepage": "https://mindforce.me"
		}
	],
	"support": {
		"issues": "https://github.com/mindforce/cakephp-passengers/issues",
		"source": "https://github.com/mindforce/cakephp-passengers"
	},
	"minimum-stability": "dev",
	"require": {
        "php": ">=5.4",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "3.0.*",
		"mindforce/cakephp-rear-engine": "*",
		"dereuromark/cakephp-tools": "cake3-dev",
		"cakephp/migrations": "*",
		"friendsofcake/authenticate": "dev-cake3",
		"cakephp/acl": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "*"
	},
	"autoload": {
		"psr-4": {
			"Passengers\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Passengers\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
		}
	}

}
