{
	"name": "mindforce/cakephp-passengers",
	"description": "CakePHP Users management plugin",
	"homepage": "http://mindforce.me",
	"keywords": ["cakephp", "ui", "users", "authentication", "authorization"],
	"type": "cakephp-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "Denys Kyselov",
			"homepage": "https://mindforce.me"
		}
	],
	"support": {
		"issues": "https://github.com/mindforce/cakephp-passengers/issues",
		"source": "https://github.com/mindforce/cakephp-passengers"
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.17",
        "cakephp/plugin-installer": "*",
		"cakephp/cakephp": ">=3.1",
		"cakephp/migrations": "*",
		"mindforce/cakephp-rear-engine": "*",
		"dereuromark/cakephp-tools": "1.0.*",
		"friendsofcake/authenticate": "dev-master",
		"cakephp/acl": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "*"
	},
	"suggest": {
		"cakephp/acl": "Allows more advanced logging of application authorization",
		"dereuromark/cakephp-tinyauth": "Simple and robust authorization plugin"
	},
	"autoload": {
		"psr-4": {
			"Passengers\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Passengers\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
		}
	}

}
